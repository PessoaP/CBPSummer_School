   RowBox[{"r_", ",", "\[Sigma]_"}], "]"}], ":=", 
  RowBox[{"4", "\[Pi]", "*", 
   RowBox[{
    RowBox[{"r", "^", "2"}], "/", 
    RowBox[{
     RowBox[{"Sqrt", "[", 
      RowBox[{"2", "\[Pi]", "*", 
       RowBox[{"\[Sigma]", "^", "2"}]}], "]"}], "^", "3"}]}], "*", 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"r", "^", "2"}]}], "/", "2"}], "/", 
     RowBox[{"\[Sigma]", "^", "2"}]}], "]"}]}]}]], "Input",
 CellLabel->"In[8]:=",ExpressionUUID->"cfa519e3-0e2d-4aab-922c-db1b0fe17274"],

