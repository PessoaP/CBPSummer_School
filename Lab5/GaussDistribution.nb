Cell["Gaussian distribution", "Section",
 CellChangeTimes->{{3.725240581584497*^9, 
  3.725240588451941*^9}},ExpressionUUID->"4197584e-0c20-447b-8299-\
5e97146f90ac"],

Cell["Here we produce mean and variance of a Gaussian distribution", "Text",
 CellChangeTimes->{{3.7252405978148203`*^9, 3.7252406148873987`*^9}},
 Background->RGBColor[
  0.94, 0.91, 0.88],ExpressionUUID->"3786a054-6215-4641-a55c-754bccfd1b2b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"g", "[", 
   RowBox[{"x_", ",", "x0_", ",", "\[Sigma]_"}], "]"}], ":=", 
  RowBox[{"Exp", "[", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "-", "x0"}], ")"}], "^", "2"}]}], "/", 
    RowBox[{"(", 
     RowBox[{"2", 
      RowBox[{"\[Sigma]", "^", "2"}]}], ")"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.725240634871653*^9, 3.725240665275816*^9}, {
  3.725240696353415*^9, 3.7252407123149443`*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"7c1fabb9-e4cd-4a3d-b098-84646cc36e6e"],

