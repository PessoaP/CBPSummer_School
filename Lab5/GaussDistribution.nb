Cell["Normalization", "Subsection",
 CellChangeTimes->{{3.72524067554832*^9, 
  3.7252406782443123`*^9}},ExpressionUUID->"9d4b8536-e2b5-4838-ab4b-\
dd5f0a5c68af"],

Cell["Normalizing the distribution", "Text",
 CellChangeTimes->{{3.725240881992188*^9, 3.725240888723925*^9}},
 Background->RGBColor[
  0.94, 0.91, 0.88],ExpressionUUID->"86996975-14f2-4f3d-9b94-c4d6737e4058"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"1", "/", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"x", ",", "x0", ",", "\[Sigma]"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
      RowBox[{"Assumptions", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"\[Sigma]", ">", "0"}], "}"}]}]}], "]"}]}]}], 
  RowBox[{"(*", 
   RowBox[{"Normalization", " ", "coeficient"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gauss2", "[", 
    RowBox[{"x_", ",", "x0_", ",", "\[Sigma]_"}], "]"}], "=", 
   RowBox[{"a", "*", 
    RowBox[{"g", "[", 
     RowBox[{"x", ",", "x0", ",", "\[Sigma]"}], "]"}]}]}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"gauss", "[", 
     RowBox[{"x_", ",", "x0_", ",", "\[Sigma]_"}], "]"}], "=", 
    RowBox[{"a", "*", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", "-", "x0"}], ")"}], "^", "2"}]}], "/", 
       RowBox[{"(", 
        RowBox[{"2", 
         RowBox[{"\[Sigma]", "^", "2"}]}], ")"}]}], "]"}]}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\< Test normalization = \>\"", ",", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"gauss2", "[", 
      RowBox[{"x", ",", "x0", ",", "\[Sigma]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\[Sigma]", ">", "0"}], "}"}]}]}], "]"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.725240720992422*^9, 3.725240851074561*^9}, 
